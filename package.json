{
  "name": "react-testing",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "gh-pages": "^3.1.0",
    "i": "^0.3.6",
    "node-sass": "^4.13.0",
    "npm": "^6.14.11",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-scripts": "3.2.0",
    "styled-components": "^5.2.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "eject": "react-scripts eject"
  },
  "jest": {
    "collectCoverageFrom": [
      "!src/App.js",
      "!src/index.js",
      "src/**/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 95,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "coverageReporters": [
      "html",
      "text"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "setupFilesAfterEnv": "./setupTests.js",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@testing-library/user-event": "^12.8.1",
    "jest-environment-jsdom-sixteen": "^1.0.3"
  }
}
